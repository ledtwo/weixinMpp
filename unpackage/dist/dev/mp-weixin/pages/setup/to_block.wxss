page {
  background-color: #F7FCFF;
  padding-bottom: 20rpx;
}
.lists {
  width: 702rpx;
  height: 136rpx;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  background-color: #fff;
  padding: 0 32rpx 0 18rpx;
  margin: 0 auto;
  margin-top: 24rpx;
}
.lists image {
  width: 84rpx;
  height: 84rpx;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  margin-right: 18rpx;
}
.lists text {
  font-size: 30rpx;
  color: #333;
}
.lists .listbtn {
  width: 164rpx;
  height: 62rpx;
  line-height: 62rpx;
  text-align: center;
  border: 1px solid #9D9FFD;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  font-size: 24rpx;
  color: #9D9FFD;
}

