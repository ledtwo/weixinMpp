page {
  background-color: #f7fcff;
  padding-bottom: 10rpx;
}
.center-v {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
  height: 100%;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.lists {
  width: 750rpx;
  padding: 24rpx;
}
.lists .listone {
  width: 100%;
  height: 220rpx;
  margin-bottom: 24rpx;
  background-color: #fff;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  padding: 20rpx 30rpx 24rpx 32rpx;
}
.lists .listone .onetop {
  width: 100%;
  font-size: 24rpx;
  color: #dad3ff;
}
.lists .listone .onetop .foncor {
  font-size: 32rpx;
  font-weight: 600;
  color: #9d9ffd;
}
.lists .listone .ontbot {
  width: 100%;
  -webkit-box-align: end;
  -webkit-align-items: flex-end;
          align-items: flex-end;
  margin-top: 50rpx;
}
.lists .listone .ontbot .botlef image {
  width: 84rpx;
  height: 84rpx;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  margin-right: 20rpx;
}
.lists .listone .ontbot .botlef .anthimg {
  font-size: 32rpx;
  color: #333;
}
.lists .listone .ontbot .botlef .anthimg text {
  font-size: 24rpx;
  margin-top: 12rpx;
  color: #bcbcbc;
}
.lists .listone .ontbot .botrig {
  font-size: 36rpx;
  font-weight: 600;
  color: #fe9749;
}

